{
  "name": "module-fed",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "start": "turbo start:live",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "npx @biomejs/biome format --write \"**/*.{ts,tsx,md}\"",
    "format:apply": "npx @biomejs/biome check --apply apps --no-errors-on-unmatched",
